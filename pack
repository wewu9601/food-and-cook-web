var express = require('express');
var router = express.Router();

/* GET home page. */
router.get('/login', function(req, res, next) {
  res.type('html');
  res.render('login');
});

router.get('/', function(req, res, next) {
  res.type('html');
  res.render('index');
});
router.get('/add', function(req, res, next) {
  res.type('html');
  res.render('add');
});

module.exports = router;
